    println("}}")

    println("Epsilon emax(Epsilon a, Epsilon b) {{")
    println("    if (a.index() == 0 && b.index() == 0) {{")
    println("        return max(get<double>(a), get<double>(b));")
    println("    }} else {{")
    println("        if (a.index() == 1) return emax(evar(get<string>(a)), b);")
    println("        if (b.index() == 1) return emax(a, evar(get<string>(b)));")
    println("        cerr << \"Unexpected epsilon variant type\" << endl;")
