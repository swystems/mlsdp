    println("        cerr << \"Unexpected epsilon variant type\" << endl;")
    println("        abort();")
    println("    }}")
    println("}}")

    println("Epsilon evar(string name) {{")
    println("    return 1.0;")
    println("}}")
    println()

    mut epsilon_context_copy = epsilon_context.mutable_snapshot()
    lattice.all_paths(epsilon_context: &mut epsilon_context_copy)

