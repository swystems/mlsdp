struct Lexer {
    input: [u8]
    offset: usize = 0
    committed_offset: usize = 0

    fn next(mut this) throws -> Token {
        .offset = .committed_offset
        let token = .peek()
        .committed_offset = .offset
        return token
    }

    fn peek(mut this) throws -> Token {
        .offset = .committed_offset
        while .offset < .input.size() {
            let c = .input[.offset]
            if c == b' ' or c == b'\t' or c == b'\n' {
                .offset += 1
                continue
            }

            return match c {
                b'0'..b'9' | b'9' => {
                    return .lex_number()
                }
                b'a'..b'z' | b'A'..b'Z' | b'_' | b'z' | b'Z' => {
                    return .lex_name()
                }
                b'(' => .consume(1, Token::LParen(index: .offset))
                b')' => .consume(1, Token::RParen(index: .offset))
                b'{' => .consume(1, Token::LBrace(index: .offset))
                b'}' => .consume(1, Token::RBrace(index: .offset))
                b'[' => .consume(1, Token::LSquare(index: .offset))
                b']' => .consume(1, Token::RSquare(index: .offset))
                b',' => .consume(1, Token::Comma(index: .offset))
                b':' => .consume(1, Token::Colon(index: .offset))
                b'*' => .consume(1, Token::Star(index: .offset))
                b'/' => .consume(1, Token::Slash(index: .offset))
                b'-' => {
                    if .offset + 1 < .input.size() {
                        if .input[.offset + 1] == b'>' {
                            return .consume(2, Token::Arrow(index: .offset))
                        }
                        if .input[.offset + 1] >= b'0' and .input[.offset + 1] <= b'9' {
                            return .lex_number()
                        }
                    }
                    yield .consume(1, Token::Dash(index: .offset))
                }
                b'+' => .consume(1, Token::Plus(index: .offset))
                b'.' => .consume(1, Token::Dot(index: .offset))
                b'=' => .consume(1, Token::Equal(index: .offset))
                b'>' => .consume(1, Token::Gt(index: .offset))
                else => {
                    eprintln("unexpected character in token stream: {:c} at offset {}", c, .offset)
                    throw Error::from_string_literal("unexpected character in token stream")
                }
            }
        }

        return Token::Eof(index: .offset)
    }

    fn lex_number(mut this) throws -> Token {
        let index = .offset
        mut value: i64 = 0
        mut is_negative = match .input[.offset] {
            b'-' => { 
                .offset += 1
                yield true
            }
            else => false
        }

        while .offset < .input.size() and .input[.offset] >= b'0' and .input[.offset] <= b'9' {
            value = value * 10 + (.input[.offset] - b'0') as! i64
            .offset += 1
        }

        if is_negative {
            value = -value
        }

        return Token::IntLiteral(index, value)
    }

    fn lex_name(mut this) throws -> Token {
        let index = .offset
        mut value = StringBuilder::create()
        while .offset < .input.size() and
            .input[.offset] >= b'a' and
            .input[.offset] <= b'z' or
            .input[.offset] >= b'A' and
            .input[.offset] <= b'Z' or
            .input[.offset] >= b'0' and
            .input[.offset] <= b'9' or
            .input[.offset] == b'_' {

            value.append(.input[.offset])
            .offset += 1
        }

        let name = value.to_string()
        return match name {
            "for" => Token::Foreach(index)
            "if" => Token::If(index)
            "else" => Token::Else(index)
            "then" => Token::Then(index)
            "in" => Token::In(index)
            "proc" => Token::Proc(index)
            "let" => Token::Let(index)
            "×" => Token::Ecks(index)
            "declassify" => Token::Declassify(index)
            "noise" => Token::Noise(index)
            "do" => Token::Do(index)
            else => Token::Name(index, value: name)
        }
    }

    fn consume(mut this, anon count: usize, anon token: Token) -> Token {
        .offset += count
        return token
    }
}

struct Parser {
    procedures: [Procedure] = []
    type_map: [TypeId:Type]
    epsilon_name_map: [String:EpsilonId] = [:]
    epsilon_map: [EpsilonId:Epsilon]
    sensitivity_context: Context<SensitivityId, f64>

    fn parse_base(
        mut this
        lexer: &mut Lexer
    ) throws -> Expression {
        let initial = lexer.peek()
        return match lexer.next() {
            IntLiteral(value) => Expression::IntLiteral(value)
            Name(value) => Expression::Variable(name: value)
            Declassify => {
                // declassify(expr, source_epsilon, target_label, target_epsilon)
                guard lexer.next() is LParen else { throw Error::from_string_literal("expected '('") }
                let expr = .parse(lexer)
                if lexer.peek() is Comma { lexer.next() }
                let source_epsilon = .parse_epsilon(lexer)
                if lexer.peek() is Comma { lexer.next() }
                guard lexer.next() is Name(value: target_label) else { throw Error::from_string_literal("expected target label") }
                if lexer.peek() is Comma { lexer.next() }
                let target_epsilon = .parse_epsilon(lexer)
                guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
                yield Expression::Declassify(
                    expr
                    source_epsilon
                    target_label: Label(name: target_label)
                    target_epsilon
                )
            }
            Noise => {
                // noise(expr, epsilon)
                guard lexer.next() is LParen else { throw Error::from_string_literal("expected '('") }
                let expr = .parse(lexer)
                if lexer.peek() is Comma { lexer.next() }
                let epsilon = .parse_epsilon(lexer)
                guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
                return Expression::Noise(expr: expr, epsilon)
            }
            LParen => {
                let inner = .parse(lexer, comma_can_be_pair: true)
                guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
                return inner
            }
            LSquare => {
                // [expr,*]
                mut elements: [Expression] = []
                loop {
                    match lexer.peek() {
                        RSquare => {
                            lexer.next()
                            break
                        }
                        Comma => {
                            lexer.next()
                            continue
                        }
                        else => {
                            elements.push(.parse(lexer))
                        }
                    }
                }
                return Expression::ListLiteral(elements)
            }
            Let => {
                guard lexer.next() is Name(value: var) else { throw Error::from_string_literal("expected name") }
                guard lexer.next() is Equal else { throw Error::from_string_literal("expected '='") }
                match lexer.peek() {
                    Proc => {
                        lexer.next()
                        guard lexer.next() is LParen else { throw Error::from_string_literal("expected '('") }
                        mut parameters: [String:SensitivityMapped<TypeId>] = [:]
                        loop {
                            let next = lexer.next()
                            match next {
                                Eof => {
                                    throw Error::from_string_literal("expected ')'")
                                }
                                RParen => {
                                    break
                                }
                                Comma => {
                                    continue
                                }
                                else => {}
                            }
                            guard next is Name(value: name) else { throw Error::from_string_literal("expected name") }
                            guard lexer.next() is Colon else { throw Error::from_string_literal("expected ':'") }
                            let type = .parse_type(lexer)
                            let new_sens = new_sensitivity(1f64, sensitivity_context: &mut .sensitivity_context)
                            parameters.set(name, SensitivityMapped(value: type, sensitivity: new_sens))
                        }
                        guard lexer.next() is Arrow else { throw Error::from_string_literal("expected '->'") }
                        let return_type = .parse_type(lexer)
                        let body = .parse(lexer)
                        .procedures.push(Procedure(name: var, parameters, return_type, body))
                        guard lexer.next() is In else { throw Error::from_string_literal("expected 'in'") }
                        return .parse(lexer)
                    }
                    else => {
                        let value = .parse(lexer)
                        guard lexer.next() is In else { throw Error::from_string_literal("expected 'in'") }
                        let body = .parse(lexer)
                        return Expression::Let(var, value, body)
                    }
                }
            }
            Foreach => {
                // for x in xs index i accum s = expr do
                guard lexer.next() is Name(value: var) else { throw Error::from_string_literal("expected name") }
                guard lexer.next() is In else { throw Error::from_string_literal("expected 'in'") }
                let list = .parse(lexer)
                guard lexer.next() is Name(
                    value: indkw
                ) and indkw == "index" else { throw Error::from_string_literal("expected 'index'") }
                guard lexer.next() is Name(value: index) else { throw Error::from_string_literal("expected name") }
                guard lexer.next() is Name(
                    value: acckw
                ) and acckw == "accum" else { throw Error::from_string_literal("expected 'accum'") }
                guard lexer.next() is Name(value: accum) else { throw Error::from_string_literal("expected name") }
                guard lexer.next() is Equal else { throw Error::from_string_literal("expected '='") }
                let accum_init = .parse(lexer)
                guard lexer.next() is Do else { throw Error::from_string_literal("expected 'in'") }
                let body = .parse(lexer)
                return Expression::ForEach(var, index, accum, accum_init, list, body)
            }
            If => {
                let condition = .parse(lexer)
                guard lexer.next() is Then else { throw Error::from_string_literal("expected 'then'") }
                let consequence = .parse(lexer)
                guard lexer.next() is Else else { throw Error::from_string_literal("expected 'else'") }
                let alternative = .parse(lexer)
                return Expression::IfThenElse(condition, consequence, alternative)
            }
            Eof => {
                throw Error::from_string_literal("unexpected EOF")
            }
            else => {
                eprintln("unexpected token: {}", initial)
                throw Error::from_string_literal("unexpected token")
            }
        }
    }

    fn parse(
        mut this
        lexer: &mut Lexer
        comma_can_be_pair: bool = false
    ) throws -> Expression {
        mut base = .parse_base(lexer)
        loop {
            match lexer.peek() {
                LSquare => {
                    // expr[index]
                    lexer.next()
                    let index = .parse(lexer)
                    guard lexer.next() is RSquare else { throw Error::from_string_literal("expected ']'") }
                    base = Expression::Subscript(expr: base, index)
                }
                Dot => {
                    // expr.index
                    lexer.next()
                    guard lexer.next() is IntLiteral(value: index) else { throw Error::from_string_literal("expected index") }
                    base = Expression::Subscript(expr: base, index: Expression::IntLiteral(value: index))
                }
                Comma => {
                    if comma_can_be_pair {
                        lexer.next()
                        let right = .parse(lexer)
                        base = Expression::Operation(op: Operation::Pair, operands: [base, right])
                    } else {
                        return base
                    }
                }
                LParen => {
                    // expr(op...)
                    lexer.next()
                    mut arguments: [Expression] = []
                    loop {
                        match lexer.peek() {
                            RParen => {
                                lexer.next()
                                break
                            }
                            Comma => {
                                lexer.next()
                                continue
                            }
                            Eof => {
                                throw Error::from_string_literal("unexpected EOF, expected ')'")
                            }
                            else => {
                                arguments.push(.parse(lexer))
                            }
                        }
                    }
                    base = Expression::Call(procedure: base, arguments)
                }
                Plus | Dash | Slash | Star | Gt => {
                    let op = lexer.next()
                    let right = .parse(lexer)
                    base = Expression::Operation(op: match op {
                        Plus => Operation::Add
                        Dash => Operation::Sub
                        Slash => Operation::Div
                        Star => Operation::Mul
                        Gt => Operation::Gt
                        else => {
                            abort()
                        }
                    }, operands: [base, right])
                }
                else => {
                    return base
                }
            }
        }
        return base
    }

    fn parse_type_base(
        mut this
        lexer: &mut Lexer
    ) throws -> TypeId => match lexer.next() {
        Name(value) => match value {
            "list" => {
                let inner = .parse_type(lexer)
                guard lexer.next() is IntLiteral(value: size) else { throw Error::from_string_literal("expected size") }
                let type_id = TypeId(id: .type_map.size() as! u64)
                .type_map.set(type_id, Type::List(type: inner, size: size as! usize))
                return type_id
            }
            "int" => {
                let type_id = TypeId(id: .type_map.size() as! u64)
                .type_map.set(type_id, Type::Int(InferredRange::Unknown))
                return type_id
            }
            else => {
                throw Error::from_string_literal("expected type")
            }
        }
        LParen => {
            let inner = .parse_type(lexer)
            guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
            return inner
        }
        else => {
            throw Error::from_string_literal("expected type")
        }
    }


    fn parse_type(
        mut this
        lexer: &mut Lexer
    ) throws -> TypeId {
        mut base = .parse_type_base(lexer)
        loop {
            match lexer.peek() {
                Ecks | Comma => {
                    lexer.next()
                    let right = .parse_type(lexer)
                    let type_id = TypeId(id: .type_map.size() as! u64)
                    .type_map.set(type_id, Type::Pair(left: base, right: right))
                    base = type_id
                }
                Name(value) => match value {
                    "lab" => {
                        lexer.next()
                        guard lexer.next() is LBrace else { throw Error::from_string_literal("expected '{'") }
                        guard lexer.next() is Name(value: label) else { throw Error::from_string_literal("expected label name") }
                        guard lexer.next() is RBrace else { throw Error::from_string_literal("expected '}'") }

                        // lab{} eps{}
                        guard lexer.next() is Name(value) and value == "eps" else { throw Error::from_string_literal("expected 'eps'") }
                        guard lexer.next() is LBrace else { throw Error::from_string_literal("expected '{'") }
                        let epsilon_id = .parse_epsilon(lexer)
                        guard lexer.next() is RBrace else { throw Error::from_string_literal("expected '}'") }

                        let type_id = TypeId(id: .type_map.size() as! u64)
                        .type_map.set(type_id, Type::TypeWithSec(type: base, label: Label(name: label), epsilon: epsilon_id))
                        base = type_id
                    }
                    else => {
                        return base
                    }
                }
                else => {
                    return base
                }
            }
        }

        // unreachable
        abort()
    }

    fn parse_epsilon(
        mut this
        lexer: &mut Lexer
    ) throws -> EpsilonId {
        // small embedded language for epsilon
        // eps :: eps_term | eps_term * eps | eps_term / eps
        // eps_term :: eps_base | eps_base + eps_term | eps_base - eps_term
        // eps_base :: name | float | int | ( eps ) | 'min'(eps, eps) | 'max'(eps, eps)

        mut initial = .parse_epsilon_term(lexer)
        loop {
            match lexer.peek() {
                Star => {
                    lexer.next()
                    let right = .parse_epsilon(lexer)
                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::Multiply(a: initial, b: right))
                    initial = epsilon_id
                }
                Slash => {
                    lexer.next()
                    let right = .parse_epsilon(lexer)
                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::Divide(a: initial, b: right))
                    initial = epsilon_id
                }
                else => {
                    return initial
                }
            }
        }

        // unreachable
        abort()
    }

    fn parse_epsilon_term(
        mut this
        lexer: &mut Lexer
    ) throws -> EpsilonId {
        // eps_term :: eps_base | eps_base + eps_term | eps_base - eps_term
        mut initial = .parse_epsilon_base(lexer)
        loop {
            match lexer.peek() {
                Plus => {
                    lexer.next()
                    let right = .parse_epsilon(lexer)
                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::Add(a: initial, b: right))
                    initial = epsilon_id
                }
                Dash => {
                    lexer.next()
                    let right = .parse_epsilon(lexer)
                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::Subtract(a: initial, b: right))
                    initial = epsilon_id
                }
                else => {
                    return initial
                }
            }
        }

        // unreachable
        abort()
    }

    fn parse_epsilon_base(
        mut this
        lexer: &mut Lexer
    ) throws -> EpsilonId {
        // eps_base :: name | float | int | ( eps ) | 'min'(eps, eps) | 'max'(eps, eps)
        match lexer.next() {
            Name(value) => match value {
                "min" => {
                    guard lexer.next() is LParen else { throw Error::from_string_literal("expected '('") }
                    let left = .parse_epsilon(lexer)
                    guard lexer.next() is Comma else { throw Error::from_string_literal("expected ','") }
                    let right = .parse_epsilon(lexer)
                    guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::Min(a: left, b: right))
                    return epsilon_id
                }
                "max" => {
                    guard lexer.next() is LParen else { throw Error::from_string_literal("expected '('") }
                    let left = .parse_epsilon(lexer)
                    guard lexer.next() is Comma else { throw Error::from_string_literal("expected ','") }
                    let right = .parse_epsilon(lexer)
                    guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::Max(a: left, b: right))
                    return epsilon_id
                }
                else => {
                    if .epsilon_name_map.contains(value) {
                        return .epsilon_name_map[value]
                    }

                    let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                    .epsilon_map.set(epsilon_id, Epsilon::RuntimeVar(name: value))
                    .epsilon_name_map.set(value, epsilon_id)
                    return epsilon_id
                }
            }
            IntLiteral(value) => {
                let epsilon_id = EpsilonId(id: .epsilon_map.size() as! u64)
                .epsilon_map.set(epsilon_id, Epsilon::Fixed(value as! f64))
                return epsilon_id
            }
            LParen => {
                let inner = .parse_epsilon(lexer)
                guard lexer.next() is RParen else { throw Error::from_string_literal("expected ')'") }
                return inner
            }
            else => {
                throw Error::from_string_literal("expected epsilon base")
            }
        }
    }
}